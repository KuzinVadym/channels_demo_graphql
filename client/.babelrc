{
  "presets": [
   "@babel/preset-env",
    "@babel/preset-react"
  ],
  "plugins": [
    "react-hot-loader/babel",
    [
      "@babel/plugin-syntax-decorators", {
        "decoratorsBeforeExport": true
      }
    ],
    "@babel/plugin-syntax-object-rest-spread",
    "@babel/plugin-syntax-class-properties",
    [
      "@babel/plugin-transform-runtime", {
        "regenerator": true,
        "useESModules": false
      }
    ]
  ]
}
